{
    "name": "FastAPI with Tracing",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-azuretools.vscode-docker"
        ],
        "settings": {
          "python.pythonPath": "/usr/local/bin/python"
        }
      }
    },
    "postCreateCommand": "pip install -r requirements.txt"
  }
  